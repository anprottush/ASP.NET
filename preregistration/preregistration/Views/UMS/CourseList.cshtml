@model IEnumerable<preregistration.DB.Cours>

@{
    ViewBag.Title = "CourseList";
}

<h2>CourseList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PreReq)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PreReq)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditCourse", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "DeleteCourse", new { id=item.Id })
        </td>
    </tr>
}

</table>
